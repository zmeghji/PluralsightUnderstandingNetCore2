@model Shared.ProposalModel
<tr>
    <td>@Model.Title</td>
    <td>@Model.Speaker</td>
    <td>
        @if (Model.Approved)
        {
            <i class="fa fa-check"></i>
        }
    </td>
    <td>
        @if (!Model.Approved)
        {
            <a asp-action="Approve" asp-route-proposalId="@Model.Id">Approve</a>
        }
    </td>
</tr>